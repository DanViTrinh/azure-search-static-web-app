{
  "trailingSlash": "auto",
  "routes": [
    {
      "route": "/search/*"
    },
    {
      "route": "/details/*"
    },
    {
      "route": "/api/*"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*", "/assets/*", "/*.ico"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}
